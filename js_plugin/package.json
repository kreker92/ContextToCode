{
	"name": "contexttocode",
	"displayName": "ContextToCode",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.showText"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.showText",
				"title": "show Text"
			}
		],
		"keybindings": [
			{
				"command": "extension.showText",
				"key": "ctrl+enter",
				"mac": "cmd+enter",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"eslint": "^5.13.0",
		"typescript": "^3.3.1",
		"vscode": "^1.1.34"
	},
	"dependencies": {
		"cheerio": "^1.0.0-rc.2",
		"express": "^4.16.4",
		"request-promise": "^4.2.4"
	}
}
